{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "rootDir": "."
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx", "test/env.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
